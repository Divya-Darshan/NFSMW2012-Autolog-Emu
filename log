while ($true) {
  Clear-Host
  Get-Date
  Get-NetTCPConnection | Select-Object OwningProcess, LocalPort, RemoteAddress, RemotePort, State
  Get-NetUDPEndpoint   | Select-Object OwningProcess, LocalAddress, LocalPort
  Start-Sleep -Seconds 2
}

